<div class="duel-top-option">
	<div class="back-btn">
		<img src="images/prev_arrow.png" id="duel-dir-home"><br>
		Back
	</div>
	<div class="logo-area">
		<img src="images/ematch_banner.png">
	</div>
</div>
<div class="home-content">
	<div class="duel-content">
		<p class="duel-lobby-text">DUEL LOBBY</p>
		<div class="duel-member-list">
			<table>
				<tbody>
					<% for(var i=0; i < data.length; i++) { %>
						<% if (data[i].id != user['id']) {%>
							<tr>
								<td class="duel-profile-image">
									<img src="images/profile.png">
								</td>
								<td class="duel-name">
									<p><%=data[i].username%></p>
								</td>
								<td class="duel-level">
									<p>Lvl 7</p>
								</td>
								<td class="duel-player-btn" data-value="<%=data[i].email%>">
									<img src="images/duel_player.png" id="duel-selected-player">
								</td>
							</tr>
						<% } %>
					<% } %>
				</tbody>
			</table>
		</div>
	</div>
</div>
