<div class="duel-top-option">
	<div class="back-btn">
		<img src="images/home.png" id="match-dir-home"><br>
	</div>
</div>
<div class="home-content">
	<div class="duel-content">
		<div class="profile-details">
			<span class="duel-lobby-text">WINNER</span>
			<p class="user-profile-name result-winner"></p>
			<div class="home-profile-img">
				<img src="images/profile.png">
				<div class="home-user-points">
					<span>
						<img src="images/trophy.png">
						<small class="result-winner-points"></small>
					</span>
				</div>
			</div>
			<div class="user-level-status">
				<span>RANK <span class="result-winner-rank"></span></span>
			</div>
		</div>
	</div>
	<div class="vanquished-content">
		<div>
			<p class="result-label"></p>
		</div>
		<div class="vanquished-opponent">
			<img src="images/profile.png">
			<p class="result-losser"></p>
			<p class="result-losser-points">1200 pts.</p>
		</div>
	</div>
	<div class="view-match-results">
		<button class="view-match">Result</button>
		<button class="send-message">Message</button>
	</div>
</div>
<div class="match-preview-overlay">
	<div class="preview-questions-list">
		<p>Match History</p>
		<img src="./images/close.png" class="pre-image-close">
	</div>
</div>
<div class="preview-result-overlay">
	<div class="preview-result animated slideInLeft">
		<img src="./images/close.png" class="hide-pr">
		<h4>BATTLE RESULT</h4>
		<img src="images/profile.png">
		<p>You win!</p>
		<div class="player-points-update animated slideInRight">
			<img src="images/trophy.png">
			<span>
				<span class="points-indicator"></span>
				<span class="player-earned-points">
					0
				</span>
			</span>
		</div>
	</div>
</div>